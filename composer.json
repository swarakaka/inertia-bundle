{
  "name": "skipthedragon/inertia-bundle",
  "description": "Inertia.js server-side adapter for Symfony 4, 5, 6 and 7.",
  "keywords": [
    "Symfony",
    "bundle",
    "inertia",
    "bridge",
    "monolith",
    "php"
  ],
  "homepage": "https://github.com/SkipTheDragon/inertia-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Vermeire",
      "email": "hannesvermeire@gmail.com"
    },
    {
      "name": "Tudorache Leonard-Valentin",
      "email": "tudorache.leonard@wyverr.com"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Rompetomp\\InertiaBundle\\Tests\\": "tests/",
      "Rompetomp\\InertiaBundle\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "autoload": {
    "psr-4": {
      "Rompetomp\\InertiaBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "require": {
    "php": "^8",
    "ext-json": "*",
    "symfony/config": "^5.1.7|^6.0|^7.0",
    "symfony/dependency-injection": "^5.1.7|^6.0|^7.0",
    "symfony/http-kernel": "^5.1.7|^6.0|^7.0",
    "symfony/twig-bundle": "^5.1.7|^6.0|^7.0",
    "symfony/serializer-pack": "^v1.3.0",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "symfony/http-client": "^5.1.7|^6.0|^7.0",
    "symfony/security-csrf": "^5.1.7|^6.0|^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "symfony/phpunit-bridge": "^5.1.7|^6.0|^7.0",
    "symfony/property-access": "^5.1|^6.0|^7.0",
    "symfony/serializer": "^5.1|^6.0|^7.0",
    "symfony/framework-bundle": "^5.1|^6.4|^7.0",
    "symfony/var-dumper": "^5.1.7|^6.0|^7.0",
    "symfony/yaml": "^5.1.7|^6.0|^7.0"
  }
}
